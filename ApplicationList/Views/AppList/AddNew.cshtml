    Operating System: <input type="text" , id="TxtOS" , name="OS" /><br />
    <hr /><br />
    Master Data Requirement: <input type="text" , id="TxtMasterDataReq" , name="MasterDataReq" /><br />
    GDPR Criticaity: <input type="text" , id="TxtGDPRCriticality" , name="GDPRCriticality" /><br />
    EMEAI Go Live Date: <input type="text" , id="TxtEMEAIGoLiveDate" , name="EMEAIGoLiveDate" /><br />
