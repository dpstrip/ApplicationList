    Fusion Strategy: <input type="text" , id="TxtFusionStrategy" , name="FusionStrategy" /><br />
    Legacy Company Originator: <input type="text" , id="TxtLegacyOriginator" , name="LegacyOriginator" /><br />
    SAP Area: <input type="text" , id="TxtSAPArea" , name="SAPArea" /><br />
    <hr /><br />
    <input type="submit" name="BtnSave" value="Save" />
    <input type="submit" name="BtnDelete" value="Delete" />
    <input type="button" name="BtnReset" value="Reset" />
</form>



