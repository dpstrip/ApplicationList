    IT Owner: <input type="text" , id="TxtITOwner" , name="ITOwner" /><br />
    Tech Owner: <input type="text" , id="TxtTechOwner" , name="TechOwner" /><br />
    Path: <input type="text" , id="TxtPath" , name="Path" /><br />
    Location Used: <input type="text" , id="TxtLocationUsed" , name="LocationUsed" /><br />
    Application Type: <input type="text" , id="TxtApplicationType" , name="ApplicationType" /><br />
